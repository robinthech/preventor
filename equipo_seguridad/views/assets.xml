<odoo>
    <template id="equipo_seguridad.assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="//script[last()]" position="after">
            <script src="http://szimek.github.io/signature_pad/js/signature_pad.umd.js"></script>
            <script src="/equipo_seguridad/static/src/js/equipos.js" type="text/javascript"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link href="/equipo_seguridad/static/src/scss/equipos.scss" type="text/scss" rel="stylesheet"/>
        </xpath>
    </template>
</odoo>
