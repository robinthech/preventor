<odoo>
    <template id="enfermedades.assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="//script[last()]" position="after">
            <script src="/enfermedades/static/src/js/enfermedades.js" type="text/javascript"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link href="/enfermedades/static/src/scss/enfermedades.scss" type="text/scss" rel="stylesheet"/>
        </xpath>
    </template>
</odoo>
