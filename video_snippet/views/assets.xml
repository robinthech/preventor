<odoo>
    <template id="video_snippet.assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="//script[last()]" position="after">
            <script src="/video_snippet/static/src/js/video.js" type="text/javascript"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link href="/video_snippet/static/src/scss/video.scss" type="text/scss" rel="stylesheet"/>
        </xpath>
    </template>
</odoo>
